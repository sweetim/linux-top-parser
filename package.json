{
  "name": "linux-top-parser",
  "version": "1.0.0",
  "description": "parser for linux top command",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./stream": "./lib/stream/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
